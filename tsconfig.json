{
  "files": [
    "assets/js/main.tsx"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "allowJs": true,
    "jsx": "react",
    "typeRoots": [
      "./node_modules/@types",
      "./assets/@types"
    ],
    "baseUrl": "./",
    "paths": {
      "*" : ["assets/types/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"]
}
